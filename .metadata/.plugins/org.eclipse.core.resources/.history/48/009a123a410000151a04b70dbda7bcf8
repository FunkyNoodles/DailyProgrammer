 *Written in Java, compiler:1.8u25
 */

package dailyProgrammer;

import java.util.ArrayList;
import java.util.Scanner;

public class May18_2015 {
	
	static ArrayList<Integer> arr = new ArrayList<Integer>();
	
	public static void main(String[] args){
		
		Scanner a = new Scanner(System.in);
		
		boolean first = true;
		int exponent = 0;
		int number = 0;
		exponent = a.nextInt();
		number = a.nextInt();
		int numP=number;
		int tempSum = 0;
		while(check(tempSum)!=-1){
			if(first){
				numP = number;
				first = false;
			}else{
				numP = tempSum;
			}
			//numP = tempSum;
			for(int i=(int) (Math.log10(number)+1);i>0;i--){
				tempSum = (int) Math.pow(numP/(int)(Math.pow(10,i-1)), exponent)+tempSum;
				//System.out.println(tempSum);
				numP = (int)(numP-((int)(numP/(Math.pow(10, i-1))))*Math.pow(10, i-1));
				//System.out.println(numP);
			}
			System.out.println(tempSum + ", ");
			if(first){
				
			}
		}
	}
	public static int check(int temp){
		for(int j=0; j<arr.size(); j++){
			if(temp==arr.get(j)){
				
			}
		}
		return temp;
	}
}
