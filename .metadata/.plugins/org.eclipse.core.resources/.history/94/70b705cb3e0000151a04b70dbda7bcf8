/*From r/DailyProgrammer
 *Link: http://www.reddit.com/r/dailyprogrammer/comments/36cyxf/20150518_challenge_215_easy_sad_cycles/
 *Written in Java, compiler:1.8u25
 */

package dailyProgrammer;

import java.util.Scanner;

public class May18_2015 {
	public static void main(String[] args){
		
		Scanner a = new Scanner(System.in);
		
		boolean first = true;
		int exponent = 0;
		int number = 0;
		exponent = a.nextInt();
		number = a.nextInt();
		int numP=number;
		int tempSum = 0;
		int endNum=(int) Math.pow(number, exponent);
		while(tempSum!=endNum){
			if(first){
				tempSum = number;
			}
			numP = tempSum;
			for(int i=(int) (Math.log10(number)+1);i>0;i--){
				tempSum = (int) Math.pow(numP/(int)(Math.pow(10,i)), exponent)+tempSum;
				numP = (int)(numP-((int)(numP/(Math.pow(10, i-1))))*Math.pow(10, i-1));
			}
			System.out.println(tempSum + ", ");
			if(first){
				endNum = tempSum;
			}
			first = false;
		}
		
	}
}
